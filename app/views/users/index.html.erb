<h1>Bem vindo</h1>

<table>
    <thead>
        <tr>
            <th>Nome</th>
            <th>E-mail</th>
        </tr>
    </thead>
    <tbody>
    <!--<% form_with(url: users, method: "delete") -->
        <% @users.each do |user| %>
        <tr>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td><%= user.password %></td>
            <td><a href="/user/<%= user.id %>/delete">Edit</a></td>
            <td><a href="/user/<%= user.id %>/delete">Delete</a></td>
        </tr>
        <% end %>
    <!--<% end %> -->
    </tbody>
</table>